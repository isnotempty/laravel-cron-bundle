{
  "name": "tolacika/laravel-cron-bundle",
  "description": "This package is a hepler for crons without crontab",
  "keywords": [
    "laravel",
    "cron",
    "supervisor",
    "command"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Tolacika",
      "email": "1866861+tolacika@users.noreply.github.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.1",
    "cron/cron": "^1.4"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Tolacika\\CronBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tolacika\\CronBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tolacika\\CronBundle\\CronBundleServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
